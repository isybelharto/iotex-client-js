{
  "name": "iotex-client-js",
  "version": "0.0.2-beta",
  "description": "IoTeX Client",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run flow && npm run cover",
    "check-coverage": "nyc check-coverage --lines 65 --functions 60 --Statements 65",
    "ava": "NODE_ENV=test ava",
    "server": "node server",
    "lint": "eslint . --fix",
    "cover": "NODE_ENV=test npm run lint && nyc npm run ava",
    "flow": "flow",
    "flow-install": "flow-typed install",
    "postinstall": "npm run flow-install",
    "size": "ls -lh dist/",
    "bp": "npm run build && npm run size",
    "build": "babel src/*.js src/**/*.js --ignore src/**/*.test.js -d dist",
    "build:watch": "babel --watch src/**/*.js -d dist",
    "prepublish": "npm run build"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/__tests__/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "cobertura"
    ],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "temp-directory": "./coverage/.nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:iotexproject/iotex-client-js.git"
  },
  "keywords": [],
  "author": "field_author",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/iotexproject/iotex-client-js/issues"
  },
  "homepage": "https://github.com/iotexproject/iotex-client-js#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-flow-strip-types": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "ava": "1.0.0-beta.8",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-react-require": "3.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "eslint": "5.5.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.80.0",
    "flow-typed": "2.4.0",
    "global": "4.3.2",
    "jsdom": "12.0.0",
    "jsdom-global": "3.0.2",
    "nyc": "13.0.1"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bech32": "^1.1.3",
    "dotty": "^0.1.0",
    "ethereumjs-abi": "^0.6.5",
    "js-sha3": "^0.8.0",
    "nkn-client": "^0.4.3",
    "solc": "^0.4.25"
  }
}
